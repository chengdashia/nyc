<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.git.bds.nyc.user.dao.user.UserDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.git.bds.nyc.user.domain.User">
        <id column="id" property="id"/>
        <result column="openid" property="openid"/>
        <result column="screen_name" property="screenName"/>
        <result column="avatar" property="screenName"/>
        <result column="create_time" property="avatar"/>
        <result column="login_time" property="loginTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , openid, screen_name, avatar, create_time, login_time
    </sql>

</mapper>
