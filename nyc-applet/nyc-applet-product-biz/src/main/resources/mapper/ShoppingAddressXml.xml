<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.git.bds.nyc.product.mapper.ShoppingAddressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.git.bds.nyc.product.model.domain.ShoppingAddress">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="consignee" property="consignee" />
        <result column="phone" property="phone" />
        <result column="location" property="location" />
        <result column="detailed_address" property="detailedAddress" />
        <result column="default" property="isDefault"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, consignee, phone, location, detailed_address, is_default
    </sql>

</mapper>
